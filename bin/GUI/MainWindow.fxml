<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.shape.Line?>
<?import javafx.scene.text.Font?>

<BorderPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.MainWindowController">
   <center>
      <AnchorPane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="carsButton" layoutX="52.0" layoutY="72.0" mnemonicParsing="false" onAction="#openCars" prefHeight="39.0" prefWidth="107.0" text="Cars">
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
            <Button fx:id="customersButton" layoutX="51.0" layoutY="121.0" mnemonicParsing="false" onAction="#OpenCustomer" text="Customers">
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
            <Button fx:id="rentalsButton" layoutX="53.0" layoutY="21.0" mnemonicParsing="false" onAction="#openRental" prefHeight="39.0" prefWidth="107.0" text="Rentals">
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
         </children>
      </AnchorPane>
   </center>
   <top>
      <Pane prefHeight="116.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <Label layoutX="17.0" layoutY="35.0" text="FCar Company" textFill="#ab1212">
               <font>
                  <Font name="Arial Bold" size="22.0" />
               </font>
            </Label>
            <Label layoutX="52.0" layoutY="61.0" text="Car Rental System" textFill="#3501a4" />
            <Line endX="199.99998474121094" endY="95.50000762939453" startY="95.5" stroke="#ab1212" strokeWidth="5.0" />
         </children>
      </Pane>
   </top>
</BorderPane>
